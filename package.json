{
  "name": "@getstation/rx-ipc-electron",
  "version": "0.2.1",
  "description": "Pass RxJS Observables through IPC in Electron",
  "main": "lib/index.js",
  "types": "lib/index.d.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "clean": "rimraf lib",
    "lint": "tslint **/*.ts -e \"lib/**/*.ts\" -e \"test/**/*.ts\" -t verbose",
    "mocha": "electron-mocha ./test/*.ts",
    "test": "npm run lint && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getstation/rx-ipc-electron.git"
  },
  "keywords": [
    "Electron",
    "IPC",
    "ipcMain",
    "ipcRenderer",
    "RxJS",
    "Observable"
  ],
  "author": "Colin Skow <colinskow@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/getstation/rx-ipc-electron/issues"
  },
  "homepage": "https://github.com/getstation/rx-ipc-electron",
  "dependencies": {
    "rxjs": "^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.5",
    "chai": "^4.1.2",
    "electron": "^1.8.4",
    "electron-mocha": "^6.0.1",
    "mocha-typescript": "^1.1.12",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "~2.7.2"
  }
}
